{
  "name": "realtime-react-client",
  "version": "1.0.0",
  "description": "React single page app for real-time data",
  "main": "src/server/server.js",
  "scripts": {
    "clean": "rm -rf build",
    "build-dev": "npm run clean && webpack --watch --progress --colors",
    "build-prod": "npm run clean && webpack -p --config webpack.production.config.js",
    "start": "webpack-dev-server --hot --inline --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hara-io/realtime-react-client.git"
  },
  "keywords": [
    "react",
    "isomorphic",
    "realtime",
    "client"
  ],
  "author": "HARA",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hara-io/realtime-react-client/issues"
  },
  "homepage": "https://github.com/hara-io/realtime-react-client",
  "dependencies": {
    "react": "^0.13.3",
    "react-router": "^0.13.3"
  },
  "devDependencies": {
    "babel": "^5.8.12",
    "babel-core": "^5.8.12",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.15.6",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "node-sass": "^3.2.0",
    "react-hot-loader": "^1.2.8",
    "sass-loader": "^1.0.3",
    "style-loader": "^0.12.3",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1"
  }
}
